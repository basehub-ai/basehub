{
  "name": "basehub",
  "description": "A very fast Headless CMS.",
  "author": "JB <jb@basehub.com>",
  "version": "8.2.11",
  "license": "MIT",
  "repository": "basehub-ai/basehub",
  "bugs": "https://github.com/basehub-ai/basehub/issues",
  "homepage": "https://basehub.com",
  "bin": {
    "basehub": "./dist/bin/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react-pump": {
      "import": "./dist/react/pump/index.js",
      "types": "./dist/react/pump/index.d.ts"
    },
    "./next-toolbar": {
      "import": "./dist/next/toolbar/index.js",
      "types": "./dist/next/toolbar/index.d.ts"
    },
    "./events": {
      "import": "./dist/events/index.js",
      "types": "./dist/events/index.d.ts"
    },
    "./workflows": {
      "import": "./dist/workflows/index.js",
      "types": "./dist/workflows/index.d.ts"
    },
    "./react-icon": {
      "import": "./dist/react/icon/index.js",
      "types": "./dist/react/icon/index.d.ts"
    },
    "./react-form": {
      "import": "./dist/react/form/index.js",
      "types": "./dist/react/form/index.d.ts"
    },
    "./react-svg": {
      "import": "./dist/react/icon/index.js",
      "types": "./dist/react/icon/index.d.ts"
    },
    "./search": {
      "import": "./dist/search/index.js",
      "types": "./dist/search/index.d.ts"
    },
    "./react-search": {
      "import": "./dist/react/search/index.js",
      "types": "./dist/react/search/index.d.ts"
    },
    "./react-rich-text": {
      "import": "./dist/react/rich-text/index.js",
      "types": "./dist/react/rich-text/index.d.ts"
    },
    "./next-image": {
      "import": "./dist/next/image/index.js",
      "types": "./dist/next/image/index.d.ts"
    },
    "./react-code-block": {
      "import": "./dist/react/code-block/index.js",
      "types": "./dist/react/code-block/index.d.ts"
    },
    "./react-code-block/client": {
      "import": "./dist/react/code-block/client/index.js",
      "types": "./dist/react/code-block/client/index.d.ts"
    },
    "./api-transaction": {
      "import": "./dist/api-transaction.js",
      "types": "./dist/api-transaction.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "emit-dts": "dts-bundle-generator --inline-declare-externals --external-inlines @basehub/mutation-api-helpers --no-check -o dts/src/api-transaction.d.ts src/api-transaction.ts && node scripts/fix-api-transaction-types.mjs && tsc --project tsconfig.dts-only.json",
    "dev": "pnpm emit-dts && pnpm build --watch",
    "build": "tsup --config tsup.config.ts",
    "yalc:push": "pnpm build && yalc push",
    "test": "vitest"
  },
  "dependencies": {
    "@basehub/genql": "9.0.0-canary.13",
    "@radix-ui/react-slot": "^1.1.0",
    "@shikijs/transformers": "1.17.7",
    "@xmldom/xmldom": "^0.9.6",
    "arg": "5.0.1",
    "dotenv-mono": "1.3.10",
    "esbuild": "0.25.5",
    "esbuild-plugin-d.ts": "1.3.1",
    "github-slugger": "^2.0.0",
    "hast-util-to-jsx-runtime": "^2.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "pusher-js": "^8.3.0",
    "resolve-pkg": "2.0.0",
    "server-only": "^0.0.1",
    "shiki": "1.17.7",
    "typesense": "^1.8.2",
    "zod": "^3.22.1"
  },
  "devDependencies": {
    "@basehub/mutation-api-helpers": "2.0.11",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.get": "4.4.9",
    "@types/node": "18.13.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "dts-bundle-generator": "^9.5.1",
    "esbuild-scss-modules-plugin": "^1.1.1",
    "next": "^13.5.3",
    "pkg-pr-new": "^0.0.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "8.0.2",
    "type-fest": "3.0.0",
    "vitest": "^3.2.3"
  }
}
